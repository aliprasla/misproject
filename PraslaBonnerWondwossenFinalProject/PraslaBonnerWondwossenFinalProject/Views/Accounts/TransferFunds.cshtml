@model PraslaBonnerWondwossenFinalProject.Models.Account
@{
    ViewBag.Title = "TransferFunds";
}

<h2>Transfer Funds</h2>

<div class="form-horizontal">
    <div class="form-group">
        From Account:
        <div class="col-md-10">
            <div class="checkbox">
                @Html.DropDownList("AccountID", (SelectList)ViewBag.allAccounts, new { @class = "form-control" })
            </div>
        </div>
    </div>

    <div class="form-group">
        To Account:
        <div class="col-md-10">
            <div class="checkbox">
                @Html.DropDownList("AccountID", (SelectList)ViewBag.allAccounts, new { @class = "form-control" })
            </div>
        </div>
    </div>

</div>

